section.pt-page(data-id='portfolio')
    .section-inner.custom-page-content
        .page-header.color-1
            h2 Portfolio
        .page-content
            // Portfolio Content
            .portfolio-content
                // Portfolio filter
                ul#portfolio_filters.portfolio-filters
                    li.active
                        a.filter.btn.btn-sm.btn-link.active(data-group='all') All
                    li
                        a.filter.btn.btn-sm.btn-link(data-group='media') Media
                    li
                        a.filter.btn.btn-sm.btn-link(data-group='illustration') Illustration
                    li
                        a.filter.btn.btn-sm.btn-link(data-group='video') Video
                // End of Portfolio filter
                // Portfolio Grid
                #portfolio_grid.portfolio-grid.portfolio-masonry.masonry-grid-3.shuffle
                    // Portfolio Item 1
                    figure.item.shuffle-item.filtered(data-groups='["all", "media"]')
                        a.ajax-page-load(href='portfolio-1.html')
                            img(src='img/portfolio/1.jpg', alt='')
                            div
                                h5.name Project Name
                                small Media
                                i.fa.fa-file-text-o
                    // /Portfolio Item 1
